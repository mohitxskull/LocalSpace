{
  "name": "@localspace/backend-core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "exports": {
    "./api": "./.adonisjs/index.ts",
    "./literals": "./types/literals.ts"
  },
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "c8 node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "ace": "node ace",
    "check": "pnpm format && pnpm typecheck && pnpm lint && pnpm test"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#modules/*": "./app/modules/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#transformers/*": "./app/transformers/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#jobs/*": "./app/jobs/*.js",
    "#miscellaneous/*": "./app/miscellaneous/*.js",
    "#types/*": "./types/*.js",
    "#cacher/*": "./app/cacher/*.js",
    "#helper/*": "./app/helper/*.js",
    "#util/*": "./app/util/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.1.2",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/tsconfig": "^1.4.1",
    "@japa/api-client": "^3.1.0",
    "@japa/assert": "^4.0.1",
    "@japa/browser-client": "^2.1.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.11.24",
    "@tuyau/utils": "^0.0.9",
    "@types/luxon": "^3.7.1",
    "@types/node": "24.3.3",
    "better-sqlite3": "^12.2.0",
    "c8": "^10.1.3",
    "eslint": "^9.34.0",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.55.0",
    "prettier": "^3.5.3",
    "ts-node-maintained": "^10.9.5",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/bouncer": "^3.1.6",
    "@adonisjs/cache": "^1.3.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/drive": "^3.4.1",
    "@adonisjs/limiter": "^2.4.0",
    "@adonisjs/lucid": "^21.8.0",
    "@adonisjs/mail": "^9.2.2",
    "@adonisjs/redis": "^9.2.0",
    "@folie/captcha": "^0.0.2",
    "@julr/adonisjs-prometheus": "^1.2.0",
    "@localspace/lib": "workspace:*",
    "@localspace/node-lib": "workspace:*",
    "@nemoventures/adonis-jobs": "^1.0.0",
    "@tuyau/core": "^0.4.2",
    "@vinejs/vine": "^3.0.1",
    "bullmq": "^5.58.5",
    "edge.js": "^6.3.0",
    "luxon": "^3.7.1",
    "mysql2": "^3.14.3",
    "reflect-metadata": "^0.2.2",
    "ulid": "^3.0.1"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
